<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>


  <servlet>
    <servlet-name>TicketController</servlet-name>
    <servlet-class>com.bhagya.practice.servlet.controller.TicketController</servlet-class>
  </servlet>

  <servlet>
    <servlet-name>PassengerController</servlet-name>
    <servlet-class>com.bhagya.practice.servlet.controller.PassengerController</servlet-class>
  </servlet>

  <servlet>
    <servlet-name>FlightController</servlet-name>
    <servlet-class>com.bhagya.practice.servlet.controller.FlightController</servlet-class>
  </servlet>

  <servlet>
    <servlet-name>BillingController</servlet-name>
    <servlet-class>com.bhagya.practice.servlet.controller.BillingController</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>ticketController</servlet-name>
    <url-pattern>ticketcontroller</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>passengerController</servlet-name>
    <url-pattern>passengercontroller</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>flightController</servlet-name>
    <url-pattern>flightcontroller</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>billingController</servlet-name>
    <url-pattern>billingcontroller</url-pattern>
  </servlet-mapping>
</web-app>
